const left = document.querySelector('left')
const rigth =document.querySelector('.rigth')
const container = document.querySelector('.container')

left.addEventListener('mouseseenter', () => container.cclassList.add('hover-left'))
left.addEventListener('mouseleave', () => container.cclassList.remove('hover-left'))

right.addEventListener('mouseseenter', () => container.cclassList.add('hover-left'))
right.addEventListener('mouseleave', () => container.cclassList.remove('hover-left'))
